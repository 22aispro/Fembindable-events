local iterations = 10000
local n = 100

runBenchmark("Disconnect() [last of 100]", iterations, function()
	local b = fembindables.new("Test")
	local conns = {}
	for i = 1, n do conns[i] = b:Connect(emptyCallback) end
  conns[n]:Disconnect()
end)

runBenchmark("Disconnect() [first of 100]", iterations, function()
		local b = fembindables.new("Test")
		local conns = {}
		for i = 1, n do conns[i] = b:Connect(emptyCallback) end
		conns[1]:Disconnect()
end)
