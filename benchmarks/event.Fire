local iterations = 20000

local b0 = fembindables.new("Test")
runBenchmark("fembindables:Fire() [0 conns]", iterations, function()
	b0:Fire()
end)

local b1 = fembindables.new("Test")
b1:Connect(emptyCallback)
runBenchmark("fembindables:Fire() [1 conn]", iterations, function()
	b1:Fire()
end)

local b100 = fembindables.new("Test")
for i = 1, 100 do b100:Connect(emptyCallback) end
  runBenchmark("fembindables:Fire() [100 conns]", iterations, function()
  b100:Fire()
end)
